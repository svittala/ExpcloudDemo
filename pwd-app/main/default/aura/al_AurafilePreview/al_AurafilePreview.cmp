<aura:component implements="flexipage:availableForAllPageTypes,lightning:availableForFlowScreens,forceCommunity:availableForAllPageTypes" access="global" >
    
<aura:attribute name="fileID"      type="String"  default="0693S000000RS7YQAW"
    description="Document ID."/>
<!--"0693S000000QLe9QAG" --> 

<aura:attribute name="description"       type="String"  default="View Rules of Behavior"
    description="Document Description"/>


<lightning:card title="File Preview">
    	<div class="slds-p-around_large">
            <lightning:fileCard fileId="{!v.fileID}" description="{!v.description}"/>
        </div>
    </lightning:card>
</aura:component>