<aura:event type="APPLICATION"
            description="Custom event to request the closing of a tab and the opening of a record" >

    <aura:attribute name="sourceId"	type="String"
                    access="global"		
                    description="Source tab ID to be closed." />
    <aura:attribute name="targetId"	type="String"
                    access="global"
                    description="Target record ID to be opened." />
    
</aura:event>